{
  "extends": [
    "./.comlint/.stylelintrc.json"
  ],
  "rules": {
    "unit-no-unknown": [
      true,
      {
        "ignoreUnits": [
          "rx"
        ]
      }
    ],
    "property-no-vendor-prefix": [
      true,
      {
        "ignoreProperties": [
          "background-clip"
        ]
      }
    ]
  }
}