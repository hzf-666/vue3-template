{
  "name": "vue3-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:js": "eslint \"**/*.{js,ts,html,vue}\" --fix",
    "lint:css": "stylelint \"**/*.{css,less,sass,scss,html,vue}\" --fix",
    "docs": "vitepress init",
    "docs:dev": "env-cmd -x vitepress dev docs --host=$VITE_HOST --port=$VITE_DOCS_PORT",
    "docs:build": "vitepress build docs",
    "docs:preview": "env-cmd -x vitepress preview docs --host=$VITE_HOST --port=$VITE_DOCS_PORT",
    "docs:serve": "env-cmd -x vitepress serve docs --port=$VITE_DOCS_PORT"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "animate.css": "^4.1.1",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "math-expression-evaluator": "^2.0.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "pinia-plugin-persistedstate": "^3.1.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-vue": "^9.11.0",
    "postcss": "^8.4.22",
    "postcss-html": "^1.5.0",
    "postcss-relaxed-unit": "^1.1.1",
    "sass": "^1.59.3",
    "sass-loader": "^13.2.0",
    "stylelint": "^14.16.1",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^28.0.0",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-vue-components": "^0.23.0",
    "vconsole": "^3.15.0",
    "vite": "^4.2.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitepress": "^1.0.0-alpha.60"
  }
}
